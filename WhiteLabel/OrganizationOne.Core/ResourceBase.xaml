<?xml version="1.0" encoding="UTF-8" ?>
<ResourceDictionary
    x:Class="OrganizationOne.Core.ResourceBase"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <Style x:Key="LoginStyle" TargetType="Label">
        <Setter Property="TextColor" Value="Red" />
        <Setter Property="Text" Value="Login:" />
    </Style>
    <Style x:Key="SenhaStyle" TargetType="Label">
        <Setter Property="TextColor" Value="Red" />
        <Setter Property="Text" Value="Password:" />
    </Style>
    <Style x:Key="ImageStyle" TargetType="Image">
        <Setter Property="Source" Value="icon.png" />
    </Style>
</ResourceDictionary>